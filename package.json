{
  "name": "aurora-kit",
  "version": "0.0.9",
  "description": "Another CSS framework.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build:less": "lessc less/aurora.less dist/aurora.css",
    "build:postcss": "postcss --use autoprefixer cssnano --no-map --output dist/aurora.css dist/aurora.css",
    "build": "npm run build:less && npm run build:postcss && tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saeedafzal/Aurora-Kit.git"
  },
  "author": "Saeed Afzal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saeedafzal/Aurora-Kit/issues"
  },
  "homepage": "https://github.com/saeedafzal/Aurora-Kit#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "cssnano": "^4.1.10",
    "less": "^4.1.1",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "typescript": "^4.2.3"
  },
  "files": [
    "dist",
    "less",
    "LICENSE",
    "README.md"
  ]
}
